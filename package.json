{
  "name": "@variate/engine",
  "version": "1.1.1",
  "description": "Variate AB testing engine for modern applications",
  "main": "dist/variate.js",
  "module": "dist/variate.esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.js",
    "dev": "npm run clean && rollup -w -c rollup.config.js",
    "clean": "rimraf dist/",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "lint:fix": "eslint --fix src/",
    "docs": "esdoc",
    "prepublishOnly": "npm run clean && npm run lint && npm run build",
    "publish": "npm publish --access public",
    "release": "standard-version",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/variateapp/variate-engine.git"
  },
  "keywords": [
    "ab-testing",
    "variate"
  ],
  "author": "Valentin Prugnaud <valentin@whatdafox.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/variateapp/variate-engine/issues"
  },
  "homepage": "https://github.com/variateapp/variate-engine#README.md",
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.23",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/get-value": "^3.0.1",
    "deepmerge": "^4.2.2",
    "get-value": "^3.0.1",
    "json-logic-js": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
